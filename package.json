{
	"name": "wardrobe-ai-enhanced",
	"version": "1.0.0",
	"description": "Enhanced Wardrobe AI - Virtual Fashion Platform with 3D Avatars and AI Styling",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"type-check": "tsc --noEmit",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:ci": "vitest run --coverage --reporter=basic",
		"backend:dev": "cd backend && npm run dev",
		"docker:build": "docker build -t wardrobe-ai .",
		"docker:dev": "docker-compose -f docker-compose.dev.yml up",
		"k8s:deploy": "kubectl apply -f k8s/",
		"start:full": "concurrently \"npm run backend:dev\" \"npm run dev\""
	},
